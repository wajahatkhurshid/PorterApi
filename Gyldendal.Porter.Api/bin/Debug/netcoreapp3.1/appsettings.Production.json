{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "HangFireConfig": {
    "HangfireJobCron": "0 0 12 1/1 * ?"
  },
  "MongoDbConfig": {
    "ConnectionString": "#{MongoDbConnectionString}#",
    "DbName": "PorterDb"
  },
  "LoggingManagerConfig": {
    "RabbitMqLogUser": "#{RabbitMqLogUser}#",
    "RabbitMqLogUserPassword": "#{RabbitMqLogUserPassword}#",
    "RabbitMqLogHost": "#{RabbitMqLogHost}#"
  },
  "DatahubApiConfig": {
    "BaseUrl": "#{DatahubApiBaseUrl}#"
  }
}
